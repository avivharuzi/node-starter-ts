{
  "name": "starter-ts",
  "version": "1.0.0",
  "description": "node.js starter files with typescript",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && nodemon dist/server.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npm:start\" \"npm:watch\"",
    "prod": "cross-env NODE_ENV=production npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avivharuzi/node-starter-ts"
  },
  "author": "“Aviv <“avivharuzi@gmail.com”> (https://www.avivharuzi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avivharuzi/node-starter-ts/issues"
  },
  "homepage": "https://github.com/avivharuzi/node-starter-ts#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/joi": "^14.0.0",
    "@types/node": "^10.12.12",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.7",
    "pm2": "^3.2.2",
    "ts-node": "^7.0.1",
    "tsc": "^1.20150623.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "joi": "^14.3.0"
  }
}
