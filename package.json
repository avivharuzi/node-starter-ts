{
  "name": "node-starter-ts",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "npm run build && nodemon dist/main.js",
    "start:dev": "cross-env NODE_ENV=development concurrently \"npm:start\" \"npm:watch\"",
    "start:prod": "cross-env NODE_ENV=production npm run start"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "http-methods-constants": "^1.0.0",
    "http-status-codes": "^1.4.0",
    "joi": "^14.3.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chalk": "^2.2.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.45",
    "@types/http-status-codes": "^1.2.0",
    "@types/joi": "^14.3.4",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.2",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.7.0",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3"
  }
}
